  <!-- Bloodborne Flame --> <!-- Need VEF Projectile -->

  <VFECore.ExpandableProjectileDef ParentName="BaseBullet">
    <defName>BloodborneRemake_Flame</defName>
    <label>fire</label>
    <thingClass>VFECore.FlamethrowProjectile</thingClass>
    <graphicData>
      <texPath>Things/Projectiles/BloodborneRemake_Flame</texPath>
      <texPathFadeOut>Things/Projectiles/BloodborneRemake_FlameFadeOut</texPathFadeOut>
      <shaderType>MoteGlow</shaderType>
    </graphicData>
    <drawOffscreen>true</drawOffscreen>
    <lifeTimeDuration>80</lifeTimeDuration>
    <widthScaleFactor>0.85</widthScaleFactor>
    <heightScaleFactor>1</heightScaleFactor>
    <startingPositionOffset>(0, 0, -1)</startingPositionOffset>
    <totalSizeScale>0.75</totalSizeScale>
    <tickFrameRate>5</tickFrameRate>
    <finalTickFrameRate>4</finalTickFrameRate>
    <tickDamageRate>20</tickDamageRate>
    <dealsDamageOnce>true</dealsDamageOnce>
    <minDistanceToAffect>3</minDistanceToAffect>
    <projectile>
      <damageDef>Flame</damageDef>    
      <flyOverhead>false</flyOverhead>
      <damageAmountBase>22</damageAmountBase>
      <stoppingPower>4.5</stoppingPower>    
      <ai_IsIncendiary>true</ai_IsIncendiary>
      <speed>16</speed>
    </projectile>
  </VFECore.ExpandableProjectileDef>

